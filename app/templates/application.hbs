{{#if session.isAuthenticated}}
  <nav>
    <ul>
      <li>
        <LinkTo @route="index">Search</LinkTo>
      </li>
      <li>
        <LinkTo @class="favorites" @route="favorites">Favorites</LinkTo>
      </li>
      <li>
        Logged in as {{ session.data.authenticated.user.displayName }}
        <button class="sign-out" {{action "logout"}}>Sign out</button>
      </li>
    </ul>
  </nav>
  {{outlet}}
{{else}}
  <button class="sign-in" {{action "login"}}>Sign in with Google</button>
{{/if}}

